    "@types/node": "^22.8.6",
    "@vitest/coverage-v8": "2.1.4",
    "@vitest/ui": "2.1.4",
    "eslint": "8.57.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.6.0",
    "globals": "15.11.0",
    "npm-run-all": "4.1.5",
